
@{
	ViewBag.Title = "TeacherIndividual";
	Layout = "~/Views/Shared/TMobileLayout/_TMobileLayout3.cshtml";
}
<style>
	div .row {
		margin:0;
		margin-top: 10px;
		padding: 0;
		width: 100%;
		background-color: #ffffff;
	}
</style>
<div class="row">
	<div class="col-xs-1 col-sm-1">
	</div>
	<div class="col-xs-11 col-sm-11">
		<h3>@ViewBag.name</h3>
		<p>@ViewBag.account</p>
	</div>
</div>

<div class="row">
	<button type="button" class="btn btn-default btn-lg btn-block" style="margin-top:10px;" onclick="window.location.href='/TeacherMobile/TeacherMyCourse'">
		我的课程
		<span class="glyphicon glyphicon-chevron-right" style="float:right"></span>
	</button>
	<button type="button" class="btn btn-default btn-lg btn-block"style="margin-top:10px;" onclick="window.location.href='/TeacherMobile/AccountAndSet' ">
		账户与设置
		<span class="glyphicon glyphicon-chevron-right" style="float:right"></span>
	</button>
</div>


