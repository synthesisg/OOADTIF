
@{
    ViewBag.Title = "MarkReport";
    Layout = "~/Views/Shared/TMobileLayout/_TMobileLayout4.cshtml";
}
<p>书面报告打分</p>
@{
    int count = ViewBag.model.list.Count;
    for (int i = 1; i <= count; i++)
    {
<div class="container" style="width:100%;background-color:#f0f0f0">
    <p style="font-weight:bold;width:20%;height:40px;line-height:40px;display:inline;margin-right:3em">第 @i  组</p>
    @if (ViewBag.model.list[i - 1].report_name == null || ViewBag.model.list[i - 1].report_name == "")
    {
        <font>未提交</font>
    }
    else
    {
        <a href="javascript:void(0);" onclick="dlMethod('@ViewBag.model.list[i - 1].report_url');" style="cursor:pointer;width:40%;">@ViewBag.model.list[i - 1].report_name</a>
    }
    <input type="text" style="width:3em" maxlength="2" name="@ViewBag.model.list[i - 1].id" value="@ViewBag.model.list[i - 1].report_score" onkeyup="value=value.replace(/[^\d]/g,'') " ng-pattern="/[^a-zA-Z]/" />
</div>
    }
}
<button type="button" class="btn btn-primary" style="width:100%;margin-top:20px;margin-bottom:20px">
    保存修改
</button>
<script>
    function dlMethod(str) {
        window.location.href = "/File/Download_stu" + str;
    }
</script>
