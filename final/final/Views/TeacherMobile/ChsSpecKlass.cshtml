
@{
	ViewBag.Title = "ChsSpecKlass";
	Layout = "~/Views/Shared/TMobileLayout/_TMobileLayout4.cshtml";
}
<div class="row" style="margin-top:20px;width:100%">
	<div class="col-xs-12" style="padding:0;margin:0;">
        @{
        int x=0;
		foreach (var klass in ViewBag.ks) {
			<div class="container" style="margin-top:10px;height:50px;background-color:#ffffff;cursor:pointer" onclick="goto(@klass.id)">
				<p id="@klass.id" style="line-height:50px;padding:0px 1em;display:inline">@ViewBag.serial[x]</p><!--或者编号-->
			</div>
            x++;
        }
        }
	</div>
</div>
<script>
	function goto(n) {
		var temp = document.getElementById(n).innerHTML;
        window.location.href = "/TeacherMobile/KlassSeminar/" + n;
	}
</script>