
@{
	ViewBag.Title = "Seminar";
	ViewBag.TitleText = "讨论课";
	Layout = "~/Views/Shared/TMobileLayout/_TMobileLayout3.cshtml";
}
<!-- 需要课程和班级信息-->
<!-- body-->
<div class="row" style="margin-top:20px;width:100%">
	<div class="col-xs-12" style="padding:0;margin:0;">
		@foreach (var item in ViewBag.colist){
			<div class="container" style="margin-top:10px;height:50px;background-color:#ffffff;cursor:pointer" onclick="goto(@item.id)">
				<p id="@item.id" style="line-height:50px;padding:0px 1em;display:inline">@item.course_name</p>
			</div>
		}
	</div>
</div>
<script>
	function goto(n) {
		var temp = document.getElementById(n).innerHTML;
		window.location.href = "ChsSpecSeminar/" + n;
	}
</script>